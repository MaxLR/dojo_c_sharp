@model ASPIntro.Models.VideosView

<a href="/second_route">Home</a>

<h2>@Model.Title</h2>

  @foreach (string videoId in Model.YoutubeVideoIds)
  {
    <iframe width="280" height="200" src="https://www.youtube.com/embed/@videoId" title="YouTube video player"
    frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
    allowfullscreen></iframe>
  }
